files <- list.files(pattern = "\\.R$", recursive = TRUE)
for(file in files) {
lines <- readLines(file)
t_lines <- grep("\\bT\\b", lines)
f_lines <- grep("\\bF\\b", lines)
if(length(t_lines) > 0 || length(f_lines) > 0) {
cat(file, "contains T/F at lines:",
paste(c(t_lines, f_lines), collapse=", "), "\n")
}
}
